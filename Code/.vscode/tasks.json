{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++.exe build active file ver(1)",
      "command": "/usr/bin/g++",
      "args": [
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.exe",
        "-Wall",
        "-Werror",
        "-Wextra",
        "-O3",
        "-fpermissive",
        "math_unsigned.cpp",
        "math_signed.cpp",
        "algorithms.cpp",
        "math_rational.cpp",
        "math_fast_rational.cpp",
        "${file}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
    ]
}